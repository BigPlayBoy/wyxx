        <div class="center">
        </div>
    </section>
    <!-- 特色当地体验 -->
    <section class="tsddty content-part">
        <div class="center">
        </div>
    </section>
    <!-- 最世界；深度旅行 -->
    <section class="zsj content-part common-style">
        <div class="center">
        </div>
    </section>
    <!-- 跟团/半自助游 -->
    <section class="gtbzz content-part common-style">
        <div class="center">
        </div>
    </section>
    <!-- 页脚 -->
    <footer>
        {{include '../../components/footer-nav/footer-nav.art'}}
        {{include '../../components/bottom-bar/bottom-bar.art'}}
        <!-- 返回顶部按钮 -->
        {{include '../../components/backtotop/backtotop.art'}}
